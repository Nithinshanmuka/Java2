package Pattren3;

public class Practicxe {

	public static void main(String[] args) {
		
		for(int i=1;i<=5;i++) {
			for(int j=i;j<=i;j++) {
				System.out.println("*");
			}
			System.out.println();
			
		}
		for(int i=5;i>=1;i--) {
			for(int j=i;j>=i;j++) {
				System.out.println("*");

		
			
		
	}
}
	}
}
